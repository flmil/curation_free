<div>
	<% @lists.each do |list| %>
		<ul class="foo">
			<li>
				<a href="<%= list[:url] %>"><img border="1" src="<%= list[:image] %>"width="150px" height="150px"></a>
			</li>
			<li>
				<a href="<%= list[:url] %>"><h6><%= list[:name] %></h6></a>
			</li>
			<li>
				<a href="<%= list[:url] %>"><h5><%= list[:money] %></h5></a>
			</li>
			<form action="delete_list" method="post">
				<button type="submit">削除</button>
			</form>
		</ul>
	<% end %>
</div>
